%% preamble
@preamble{"\providecommand{\glslink}[2]{#2}"}
@preamble{"\providecommand{\pythonil}[1]{\texttt{#1}}"}
@preamble{"\providecommand{\pythonIdx}[1]{}"}
@preamble{"\providecommand{\pythonilIdx}[1]{\pythonil{#1}}"}
@preamble{"\providecommand{\sqlil}[1]{\texttt{#1}}"}
@preamble{"\providecommand{\sqlIdx}[1]{}"}
@preamble{"\providecommand{\sqlilIdx}[1]{\sqlil{#1}}"}
@preamble{"\providecommand{\inQuotes}[1]{``#1''}"}
@preamble{"\providecommand{\githubUser}[1]{\inQuotes{\href{https://github.com/#1}{#1}}}"}

%% basic shortcuts
@string { and = " and " }
@string { andothers = and # "others" }
